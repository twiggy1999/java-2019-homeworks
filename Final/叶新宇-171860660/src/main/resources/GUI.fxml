<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane fx:id="Pane" prefHeight="720.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GameModule.Controller">
    <children>
        <GridPane layoutX="0.0" layoutY="0.0" prefHeight="720.0" prefWidth="1200.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="960.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="240.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="91.0" prefHeight="91.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="91.0" prefHeight="91.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="91.0" prefHeight="91.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="91.0" prefHeight="91.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="91.0" prefHeight="91.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="91.0" prefHeight="91.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="174.0" vgrow="SOMETIMES" />
                <RowConstraints />
            </rowConstraints>
            <children>
                <Button fx:id="startGame" mnemonicParsing="false" onAction="#startGameHandler" prefHeight="50.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="0">
                    <GridPane.margin>
                        <Insets left="35.0" />
                    </GridPane.margin>
                </Button>
                <Button fx:id="endGame" mnemonicParsing="false" onAction="#endGameHandler"  prefHeight="50.0" prefWidth="200.0" GridPane.columnIndex="1"  GridPane.rowIndex="1" >
                    <GridPane.margin>
                        <Insets left="35.0" />
                    </GridPane.margin>
                </Button>
                <Button fx:id="calabashCrewChangeTactic" mnemonicParsing="false" onAction="#calabashCrewChangeTacticHandler" prefHeight="50.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets left="35.0" />
                    </GridPane.margin>
                </Button>
                <Button fx:id="monsterCrewChangeTactic" mnemonicParsing="false" onAction="#monsterCrewChangeTacticHandler"  prefHeight="50.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
                    <GridPane.margin>
                        <Insets left="35.0" />
                    </GridPane.margin>
                </Button>
                <Button fx:id="loadRep" mnemonicParsing="false" onAction="#loadRepHandler"  prefHeight="50.0" prefWidth="200.0" text="Button" GridPane.columnIndex="1" GridPane.rowIndex="4">
                    <GridPane.margin>
                        <Insets left="35.0" />
                    </GridPane.margin>
                </Button>
                <Canvas fx:id="gameArea" height="540.0" width="960.0" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.columnSpan="1" GridPane.rowSpan="6"/>
                <TextArea fx:id="gameLog" prefHeight="110.0" prefWidth="960.0" GridPane.columnIndex="0" GridPane.rowIndex="6" GridPane.columnSpan="2" GridPane.rowSpan="1"/>
            </children>
        </GridPane>
    </children>
</Pane>
