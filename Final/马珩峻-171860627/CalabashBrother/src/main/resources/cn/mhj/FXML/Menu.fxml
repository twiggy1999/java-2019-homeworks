<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane id="Menu" scaleShape="false"
  stylesheets="@../CSS/MenuStyle.css" xmlns="http://javafx.com/javafx/8.0.231"
  xmlns:fx="http://javafx.com/fxml/1" fx:controller="cn.mhj.Controller.MenuController">
  <children>
    <ImageView layoutX="200.0" layoutY="35.0" pickOnBounds="true"
      preserveRatio="true">
      <image>
        <Image url="@../pic/Menu/logo.png"/>
      </image>
    </ImageView>
    <Button id="startBtn" fx:id="startBtn" layoutX="500.0" layoutY="300.0" mnemonicParsing="false"
      onMouseClicked="#startBtnClicked" onMouseEntered="#startBtnEntered"
      onMouseExited="#startBtnExited"/>
    <!--    <Button id="replayBtn" fx:id="replayBtn" layoutX="550" layoutY="350" mnemonicParsing="false"-->
    <!--      onMouseClicked="#replyBtnClicked" onMouseEntered="#replayBtnEntered"-->
    <!--      onMouseExited="#replayBtnExited"/>-->
    <Button id="quitBtn" fx:id="quitBtn" layoutX="600" layoutY="400" mnemonicParsing="false"
      onMouseClicked="#quitBtnClicked" onMouseEntered="#quitBtnEntered"
      onMouseExited="#quitBtnExited"/>
  </children>

</AnchorPane>
